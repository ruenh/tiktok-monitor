{
  "name": "tiktok-monitor",
  "version": "1.0.0",
  "description": "TikTok author monitoring system with n8n webhook integration",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:web": "cd web && npm run build",
    "build:all": "npm run build && npm run build:web",
    "start": "node dist/index.js",
    "start:web": "node dist/index.js web",
    "dev": "tsx src/index.ts",
    "dev:web": "tsx src/index.ts web",
    "test": "vitest --run",
    "test:watch": "vitest"
  },
  "keywords": [
    "tiktok",
    "monitor",
    "webhook",
    "n8n"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/cors": "^2.8.19",
    "@types/express": "^5.0.6",
    "@types/node": "^20.10.0",
    "fast-check": "^3.14.0",
    "tsx": "^4.6.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  },
  "dependencies": {
    "axios": "^1.6.0",
    "cors": "^2.8.5",
    "express": "^5.2.1"
  }
}
